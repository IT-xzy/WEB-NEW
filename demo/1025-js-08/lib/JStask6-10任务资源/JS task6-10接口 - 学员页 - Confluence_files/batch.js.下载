  output.append('<ul class="space-types"><li><p>', soy.$$escapeHtml("\u56e2\u961f"), '</p></li><li><p>', soy.$$escapeHtml("\u9879\u76ee"), '</p></li><li><p>', soy.$$escapeHtml("\u516c\u53f8"), '</p></li></ul><div class="space-description"><h1>', soy.$$escapeHtml("Confluence\u7a7a\u95f4\u662f\u6240\u6709\u5185\u5bb9\u7684\u9996\u9875\r\n"), '</h1><ul><li>', soy.$$escapeHtml("Create as many spaces as you like and use them for teams, projects and departments within your company."), '</p></li><li>', soy.$$escapeHtml("\u901a\u8fc7\u9875\u9762\u589e\u52a0\u6587\u6863\u3001\u6587\u4ef6\u3001\u7167\u7247\u5230\u4f60\u7684\u7a7a\u95f4\uff0c\u5e76\u4f7f\u5b83\u4eec\u6309\u5c42\u7ea7\u7ec4\u7ec7\u3002"), '</p></li><li>', soy.$$escapeHtml("\u5728\u7a7a\u95f4\u4e2d\u4f7f\u7528\u535a\u5ba2\u6765\u5206\u4eab\u65b0\u95fb\u6216\u53d1\u5e03\u516c\u544a\u3002"), '</p></li></ul></div>');
  return opt_sb ? '' : output.toString();
};

} catch (err) {
    if (console && console.log && console.error) {
        console.log("Error running batched script.");
        console.error(err);
    }
}


try {
/* module-key = 'com.atlassian.confluence.plugins.confluence-create-content-plugin:space-blueprints', location = 'com/atlassian/confluence/plugins/createcontent/js/space-dialog-wizard-common.js' */
AJS.toInit(function(g){var n,l,b,k,f,m,p=100;var h={spaceKey:function(){var q=g.Deferred();var r=g.trim(n.val());if(!r){q.reject("\u4F60\u5FC5\u987B\u8F93\u5165\u7A7A\u95F4\u5173\u952E\u5B57\u3002");return q}if(m){AJS.log("Aborting previous space key validation request.");m.abort()}m=g.getJSON(AJS.Confluence.getContextPath()+"/ajax/spaceavailable.action",{key:r}).done(function(s){if(!s.available&&s.message){q.reject(s.message)}else{q.resolve()}m=false});return q.promise()},name:function(){var q=g.Deferred();if(!g.trim(l.val())){q.reject("\u4F60\u5FC5\u987B\u8F93\u5165\u4E00\u4E2A\u7A7A\u95F4\u540D\u5B57\u3002")}else{q.resolve()}return q.promise()}};function d(t){var s=t[0];if(s.setSelectionRange){var r=t.val().length;s.setSelectionRange(0,r)}else{if(s.createTextRange){var q=s.createTextRange();q.execCommand("SelectAll");q.select()}}}function a(q){if(q){b.removeAttr("disabled")}else{b.attr("disabled","disabled")}}var j={};var i=0;function c(s){i++;a(false);var q=s.attr("name");var r=s.siblings(".error");j[q]=true;var t=h[q]();t.done(function(){delete j[q];r.text("").addClass("hidden")});t.fail(function(u){r.text(u).removeClass("hidden")});t.always(function(){i--;if(i==0){a(g.isEmptyObject(j))}s.attr("data-validated-value",s.val())});return t}function o(q){a(false);n=q.find("input[name='spaceKey']").first();l=q.find("input[name='name']").first();k=n.parent().find(".error");f=l.parent().find(".error");n.generateFrom(l,{uppercase:false,maxNameLength:255,maxKeyLength:255,timeoutMS:p,validationCallback:function(){n.keyup()}});function r(){var t=g(this);var s=t.val();if(typeof t.data("originalVal")==="undefined"){t.data("originalVal","")}if(t.data("originalVal")===s){return}t.data("originalVal",s);c(t)}n.keyup(g.debounce(r,500));l.keyup(r);n.focus(function(){d(n)});q.find("#permission-private").change(function(){g("#permissions-group").toggleClass("locked")})}var e={live:true,gravity:"n",title:"data-tooltip",delayIn:250,delayOut:0};g(".common-space-form .aui-iconfont-help").tooltip(e);Confluence.SpaceBlueprint.CommonWizardBindings={preRender:function(r,q){q.soyRenderContext.atlToken=AJS.Meta.get("atl-token");q.soyRenderContext.showSpacePermission=true},postRender:function(r,q){b=g(".create-dialog-create-button:visible");o(q.$container)},submit:function(r,q){setTimeout(function(){var t=c(n);var s=c(l);g.when(t,s).then(q.validationDeferred.resolve,q.validationDeferred.reject)},p+1);q.async=true;return false}};AJS.trigger("blueprint.wizard-register.ready")});
